{
    "name": "awjudd/pdo-wrapper",
    "description": "A simple PDO database wrapper",
    "keywords": ["php", "database", "pdo", "wrapper"],
    "type": "library",
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Awjudd\\PDO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Awjudd\\PDO\\Tests\\": "tests/"
        }
    },
    
